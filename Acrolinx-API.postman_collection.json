				{
					"name": "User Generated API Tokens",
					"item": [
						{
							"name": "Create an API Token (Self) with Basic auth",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"if(pm.response.code == 200){\r",
											"    postman.setEnvironmentVariable(\"ACCESS_TOKEN\", JSON.parse(responseBody).data.token);\r",
											"}"
										],
										"type": "text/javascript"
									}
								}
							],
							"request": {
								"auth": {
									"type": "noauth"
								},
								"method": "POST",
								"header": [
									{
										"key": "X-Acrolinx-Auth",
										"value": "Basic {{base64_username_password}}",
										"description": "Credentials `username:password` must be base64 encoded."
									},
									{
										"key": "Content-Type",
										"value": "application/json"
									}
								],
								"url": {
									"raw": "{{ACROLINX_URL}}/api/v1/user/self/tokens",
									"host": [
										"{{ACROLINX_URL}}"
									],
									"path": [
										"api",
										"v1",
										"user",
										"self",
										"tokens"
									]
								},
								"description": "This method provides ability to create API Token for the currently authorized user.\n\nNote: The test script automatically updates the `ACCESS_TOKEN` environment variable on success."
